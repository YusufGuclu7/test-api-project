generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Record {
  id         Int     @id @default(autoincrement())
  externalId String  @unique
  code       String
  debt       Float
  credit     Float
  rawData    Json
}